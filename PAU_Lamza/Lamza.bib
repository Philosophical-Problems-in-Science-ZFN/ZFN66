
@article{adl_new_2005,
	title = {The new higher level classification of eukaryotes with emphasis on the taxonomy of protists},
	volume = {52},
	issn = {1066-5234},
	url = {https://onlinelibrary.wiley.com/doi/full/10.1111/j.1550-7408.2005.00053.x},
	urldate = {2019-05-21},
	doi = {10.1111/j.1550-7408.2005.00053.x},
	abstract = {This revision of the classification of unicellular eukaryotes updates that of Levine et al. (1980) for the protozoa and expands it to include other protists. Whereas the previous revision was primarily to incorporate the results of ultrastructural studies, this revision incorporates results from both ultrastructural research since 1980 and molecular phylogenetic studies. We propose a scheme that is based on nameless ranked systematics. The vocabulary of the taxonomy is updated, particularly to clarify the naming of groups that have been repositioned. We recognize six clusters of eukaryotes that may represent the basic groupings similar to traditional "kingdoms." The multicellular lineages emerged from within monophyletic protist lineages: animals and fungi from Opisthokonta, plants from Archaeplastida, and brown algae from Stramenopiles.},
	language = {eng},
	number = {5},
	journal = {The Journal of Eukaryotic Microbiology},
	author = {Adl, Sina M. and Simpson, Alastair G. B. and Farmer, Mark A. and Andersen, Robert A. and Anderson, O. Roger and Barta, John R. and Bowser, Samuel S. and Brugerolle, Guy and Fensome, Robert A. and Fredericq, Suzanne and James, Timothy Y. and Karpov, Sergei and Kugrens, Paul and Krug, John and Lane, Christopher E. and Lewis, Louise A. and Lodge, Jean and Lynn, Denis H. and Mann, David G. and McCourt, Richard M. and Mendoza, Leonel and Moestrup, Ojvind and Mozley-Standridge, Sharon E. and Nerad, Thomas A. and Shearer, Carol A. and Smirnov, Alexey V. and Spiegel, Frederick W. and Taylor, Max F. J. R.},
	year = {2005},
	pmid = {16248873},
	keywords = {Animal Population Groups, Animals, Eukaryota, Eukaryotic Cells, Fungi, Plankton},
	pages = {399--451},
	file = {Pełny tekst:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/KS24ZTJN/Adl i in. - 2005 - The new higher level classification of eukaryotes .pdf:application/pdf}
}

@article{adl_revised_2012,
	title = {The revised classification of eukaryotes},
	volume = {59},
	issn = {1550-7408},
	doi = {10.1111/j.1550-7408.2012.00644.x},
	url = {https://onlinelibrary.wiley.com/doi/epdf/10.1111/j.1550-7408.2012.00644.x},
	urldate = {2019-05-21},
	abstract = {This revision of the classification of eukaryotes, which updates that of Adl et al. [J. Eukaryot. Microbiol. 52 (2005) 399], retains an emphasis on the protists and incorporates changes since 2005 that have resolved nodes and branches in phylogenetic trees. Whereas the previous revision was successful in re-introducing name stability to the classification, this revision provides a classification for lineages that were then still unresolved. The supergroups have withstood phylogenetic hypothesis testing with some modifications, but despite some progress, problematic nodes at the base of the eukaryotic tree still remain to be statistically resolved. Looking forward, subsequent transformations to our understanding of the diversity of life will be from the discovery of novel lineages in previously under-sampled areas and from environmental genomic information.},
	language = {eng},
	number = {5},
	journal = {The Journal of Eukaryotic Microbiology},
	author = {Adl, Sina M. and Simpson, Alastair G. B. and Lane, Christopher E. and Lukeš, Julius and Bass, David and Bowser, Samuel S. and Brown, Matthew W. and Burki, Fabien and Dunthorn, Micah and Hampl, Vladimir and Heiss, Aaron and Hoppenrath, Mona and Lara, Enrique and Le Gall, Line and Lynn, Denis H. and McManus, Hilary and Mitchell, Edward A. D. and Mozley-Stanridge, Sharon E. and Parfrey, Laura W. and Pawlowski, Jan and Rueckert, Sonja and Shadwick, Laura and Shadwick, Lora and Schoch, Conrad L. and Smirnov, Alexey and Spiegel, Frederick W.},
	month = sep,
	year = {2012},
	pmid = {23020233},
	pmcid = {PMC3483872},
	keywords = {Eukaryota, Genome, Phylogeny, Terminology as Topic},
	pages = {429--493},
	file = {Pełny tekst:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/2YMX6SXZ/Adl i in. - 2012 - The revised classification of eukaryotes.pdf:application/pdf}
