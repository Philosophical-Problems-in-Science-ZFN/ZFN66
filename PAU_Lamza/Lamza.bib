	pmid = {11062127},
	keywords = {Actins, Amino Acid Sequence, Animals, Eukaryotic Cells, Evolution, Molecular, Peptide Elongation Factor 1, Phylogeny, Plants, RNA, Ribosomal, Sequence Analysis, Protein, Tubulin},
	pages = {972--977}
