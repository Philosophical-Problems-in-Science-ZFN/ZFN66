	pages = {421--441},
	file = {Bechtel W.,  Abrahamsen A., Explanation a mechanist alternative, „Studies in History and Philosophy of Science Part C Studies in History and Philosophy of Biological and Biomedical S.pdf:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/88J3CZQ2/Bechtel W.,  Abrahamsen A., Explanation a mechanist alternative, „Studies in History and Philosophy of Science Part C Studies in History and Philosophy of Biological and Biomed.pdf:application/pdf;ScienceDirect Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/7E2MPA72/S1369848605000269.html:text/html}
