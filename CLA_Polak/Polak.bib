@article{rohwer_viral_2013,
	title = {Viral information},
	volume = {28},
	issn = {0169-3867, 1572-8404},
	url = {http://link.springer.com/article/10.1007/s10539-012-9344-0},
	doi = {10.1007/s10539-012-9344-0},
	abstract = {Viruses are major drivers of global biogeochemistry and the etiological agents of many diseases. They are also the winners in the game of life: there are more viruses on the planet than cellular organisms and they encode most of the genetic diversity on the planet. In fact, it is reasonable to view life as a viral incubator. Nevertheless, most ecological and evolutionary theories were developed, and continue to be developed, without considering the virosphere. This means these theories need to be to reinterpreted in light of viral knowledge or we need to develop new theory from the viral point-of-view. Here we briefly introduce our viral planet and then address a major outstanding question in biology: why is most of life viral? A key insight is that during an infection cycle the original virus is completely broken down and only the associated information is passed on to the next generation. This is different for cellular organisms, which must pass on some physical part of themselves from generation to generation. Based on this premise, it is proposed that the thermodynamic consequences of physical information (e.g., Landauer’s principle) are observed in natural viral populations. This link between physical and genetic information is then used to develop the Viral Information Hypothesis, which states that genetic information replicates itself to the detriment of system energy efficiency (i.e., is viral in nature). Finally, we show how viral information can be tested, and illustrate how this novel view can explain existing ecological and evolutionary theories from more fundamental principles.},
	language = {en},
	number = {2},
	urldate = {2016-05-04},
	journal = {Biology \& Philosophy},
	author = {Rohwer, Forest and Barott, Katie},
	month = mar,
	year = {2013},
	pages = {283--297},
	file = {Full Text PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/WCJQCRED/Rohwer i Barott - 2012 - Viral information.pdf:application/pdf;Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/TXPB2G2X/s10539-012-9344-0.html:text/html}
}

