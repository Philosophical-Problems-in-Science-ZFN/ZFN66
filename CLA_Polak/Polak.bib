	author = {Meinong, Alexius and Twardowski, Kazimierz},
	month = oct,
	year = {2016},
	note = {Google-Books-ID: WAVEDQAAQBAJ},
	keywords = {Philosophy / General, Philosophy / Epistemology, Philosophy / Metaphysics}
}

