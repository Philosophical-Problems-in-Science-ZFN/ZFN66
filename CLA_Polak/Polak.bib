	author = {Leitch, E. M. and Kovac, J. M. and Pryke, C. and Carlstrom, J. E. and Halverson, N. W. and Holzapfel, W. L. and Dragovan, M. and Reddall, B. and Sandberg, E. S.},
	month = dec,
	year = {2002},
	pages = {763--771},
	file = {Przesłana wersja:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/LBTM4GJZ/Leitch i in. - 2002 - Measurement of polarization with the Degree Angula.pdf:application/pdf;Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/23QQQ6FB/nature01271.html:text/html}
}

