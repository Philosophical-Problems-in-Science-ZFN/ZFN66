@article{polak_siec_2015,
	title = {Sieć, software czy obliczenia naturalne? {Jakie} techniki definiują myślenie filozoficzne {Homo} informaticus?},
	issn = {0039-324X},
	shorttitle = {Sieć, software czy obliczenia naturalne?},
	url = {http://studiametodologiczne.amu.edu.pl/wp-content/uploads/2016/02/SM34-08.pdf},
	abstract = {30 years ago J.D. Bolter coined the term 'defining technology'. It is the technology which shapes culture and the mentality of human beings. Defining technology describes the role of technology as a source of cognitive metaphors. We are interesting in the metaphors which have an influence on the philosophical thinking.

Bolter argued that digital computer was a defining technology of the late 20th century. He created also the the concept of 'Turing's Man'. It is a man shaped by interactions with digital computers. 

Bolter's book has appeared when the computer networks were not popular and were not global. J.C.R. Licklider's ideas were known only by specialists.  

Globalisation of networking demands the redefinition of Bolter's concept. We could use the term 'Licklider's Man' instead of 'Turing's Man'. 

We should pose the question, if the networking is the sole contemporary defining technology. Do the software or services could be a defining technology (e.g. social network service)? Does the natural computing could change our concept of defining technology?},
	language = {pl},
	number = {34},
	journal = {Studia Metodologiczne},
	author = {Polak, Paweł},
	year = {2015},
	note = {1.39 arkusza},
	pages = {143--170},
	file = {Polak P., Sieć, software czy obliczenia naturalne Jakie techniki definiują myślenie filozoficzne Homo informaticus, „Studia Metodologiczne”, nr 34, 2015, s. 143–170.pdf:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/WNUC58WK/Polak P., Sieć, software czy obliczenia naturalne Jakie techniki definiują myślenie filozoficzne Homo informaticus, „Studia Metodologiczne”, nr 34, 2015, s. 143–170.pdf:application/pdf}
}

