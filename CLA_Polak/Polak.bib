	pages = {75--84},
	file = {Lejinse, N., et al., 2012. Diffusion inside living human cells, European Physical Journal Special Topics, 204, s. 75-84.pdf:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/MQGD3DEI/Lejinse, N., et al., 2012. Diffusion inside living human cells, European Physical Journal Special Topics, 204, s. 75-84.pdf:application/pdf}
