@article{planck_collaboration_i_planck_2018,
	title = {Planck 2018 results. {I}. {Overview} and the cosmological legacy of {Planck} [in press]},
	url = {http://arxiv.org/abs/1807.06205},
	abstract = {The European Space Agency's Planck satellite, which was dedicated to studying the early Universe and its subsequent evolution, was launched on 14 May 2009. It scanned the microwave and submillimetre sky continuously between 12 August 2009 and 23 October 2013, producing deep, high-resolution, all-sky maps in nine frequency bands from 30 to 857GHz. This paper presents the cosmological legacy of Planck, which currently provides our strongest constraints on the parameters of the standard cosmological model and some of the tightest limits available on deviations from that model. The 6-parameter LCDM model continues to provide an excellent fit to the cosmic microwave background data at high and low redshift, describing the cosmological information in over a billion map pixels with just six parameters. With 18 peaks in the temperature and polarization angular power spectra constrained well, Planck measures five of the six parameters to better than 1\% (simultaneously), with the best-determined parameter (theta\_*) now known to 0.03\%. We describe the multi-component sky as seen by Planck, the success of the LCDM model, and the connection to lower-redshift probes of structure formation. We also give a comprehensive summary of the major changes introduced in this 2018 release. The Planck data, alone and in combination with other probes, provide stringent constraints on our models of the early Universe and the large-scale structure within which all astrophysical objects form and evolve. We discuss some lessons learned from the Planck mission, and highlight areas ripe for further experimental advances.},
	urldate = {2018-11-28},
	journal = {arXiv:1807.06205 [astro-ph]},
	author = {Planck Collaboration I},
	month = jul,
	year = {2018},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	file = {arXiv\:1807.06205 PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/M69VLJCQ/Planck Collaboration i in. - 2018 - Planck 2018 results. I. Overview and the cosmologi.pdf:application/pdf;arXiv.org Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/XN2DKJ8W/1807.html:text/html}
}

@article{planck_collaboration_vi_planck_2018,
	title = {Planck 2018 results. {VI}. {Cosmological} parameters [in press]},
	url = {http://arxiv.org/abs/1807.06209},
	abstract = {We present cosmological parameter results from the final full-mission Planck measurements of the CMB anisotropies. We find good consistency with the standard spatially-flat 6-parameter \${\textbackslash}textbackslashLambda\$CDM cosmology having a power-law spectrum of adiabatic scalar perturbations (denoted "base \${\textbackslash}textbackslashLambda\$CDM" in this paper), from polarization, temperature, and lensing, separately and in combination. A combined analysis gives dark matter density \${\textbackslash}textbackslashOmega\_c hˆ2 = 0.120{\textbackslash}textbackslashpm 0.001\$, baryon density \${\textbackslash}textbackslashOmega\_b hˆ2 = 0.0224{\textbackslash}textbackslashpm 0.0001\$, scalar spectral index \$n\_s = 0.965{\textbackslash}textbackslashpm 0.004\$, and optical depth \${\textbackslash}textbackslashtau = 0.054{\textbackslash}textbackslashpm 0.007\$ (in this abstract we quote \$68{\textbackslash}textbackslash,{\textbackslash}textbackslash\%\$ confidence regions on measured parameters and \$95{\textbackslash}textbackslash,{\textbackslash}textbackslash\%\$ on upper limits). The angular acoustic scale is measured to \$0.03{\textbackslash}textbackslash,{\textbackslash}textbackslash\%\$ precision, with \$100{\textbackslash}textbackslashtheta\_*=1.0411{\textbackslash}textbackslashpm 0.0003\$. These results are only weakly dependent on the cosmological model and remain stable, with somewhat increased errors, in many commonly considered extensions. Assuming the base-\${\textbackslash}textbackslashLambda\$CDM cosmology, the inferred late-Universe parameters are: Hubble constant \$H\_0 = (67.4{\textbackslash}textbackslashpm 0.5)\$km/s/Mpc; matter density parameter \${\textbackslash}textbackslashOmega\_m = 0.315{\textbackslash}textbackslashpm 0.007\$; and matter fluctuation amplitude \${\textbackslash}textbackslashsigma\_8 = 0.811{\textbackslash}textbackslashpm 0.006\$. We find no compelling evidence for extensions to the base-\${\textbackslash}textbackslashLambda\$CDM model. Combining with BAO we constrain the effective extra relativistic degrees of freedom to be \$N\_\{{\textbackslash}textbackslashrm eff\} = 2.99{\textbackslash}textbackslashpm 0.17\$, and the neutrino mass is tightly constrained to \${\textbackslash}textbackslashsum m\_{\textbackslash}textbackslashnu{\textbackslash}textless 0.12\$eV. The CMB spectra continue to prefer higher lensing amplitudes than predicted in base -\${\textbackslash}textbackslashLambda\$CDM at over \$2{\textbackslash}textbackslash,{\textbackslash}textbackslashsigma\$, which pulls some parameters that affect the lensing amplitude away from the base-\${\textbackslash}textbackslashLambda\$CDM model; however, this is not supported by the lensing reconstruction or (in models that also change the background geometry) BAO data. (Abridged)},
	urldate = {2018-11-28},
	journal = {arXiv:1807.06209 [astro-ph]},
	author = {Planck Collaboration VI},
	month = jul,
	year = {2018},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	file = {arXiv\:1807.06209 PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/8IF86KDM/Planck Collaboration i in. - 2018 - Planck 2018 results. VI. Cosmological parameters.pdf:application/pdf;arXiv.org Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/V5ECG354/1807.html:text/html}
}

@article{planck_collaboration_viii_planck_2018,
	title = {Planck 2018 results. {VIII}. {Gravitational} lensing [in press]},
	url = {http://arxiv.org/abs/1807.06210},
	abstract = {We present measurements of the cosmic microwave background (CMB) lensing potential using the final \${\textbackslash}textbackslashtextit\{Planck\}\$ 2018 temperature and polarization data. We increase the significance of the detection of lensing in the polarization maps from \$5{\textbackslash}textbackslash,{\textbackslash}textbackslashsigma\$ to \$9{\textbackslash}textbackslash,{\textbackslash}textbackslashsigma\$. Combined with temperature, lensing is detected at \$40{\textbackslash}textbackslash,{\textbackslash}textbackslashsigma\$. We present an extensive set of tests of the robustness of the lensing-potential power spectrum, and construct a minimum-variance estimator likelihood over lensing multipoles \$8 {\textbackslash}textbackslashle L {\textbackslash}textbackslashle 400\$. We find good consistency between lensing constraints and the results from the \${\textbackslash}textbackslashtextit\{Planck\}\$ CMB power spectra within the \${\textbackslash}textbackslashrm\{{\textbackslash}textbackslashLambda CDM\}\$ model. Combined with baryon density and other weak priors, the lensing analysis alone constrains \${\textbackslash}textbackslashsigma\_8 {\textbackslash}textbackslashOmega\_\{{\textbackslash}textbackslashrm m\}ˆ\{0.25\}=0.589{\textbackslash}textbackslashpm 0.020\$ (\$1{\textbackslash}textbackslash,{\textbackslash}textbackslashsigma\$ errors). Also combining with baryon acoustic oscillation (BAO) data, we find tight individual parameter constraints, \${\textbackslash}textbackslashsigma\_8=0.811{\textbackslash}textbackslashpm0.019\$, \$H\_0=67.9\_\{-1.3\}ˆ\{+1.2\}{\textbackslash}textbackslash,{\textbackslash}textbackslashtext\{km\}{\textbackslash}textbackslash,{\textbackslash}textbackslashtext\{s\}ˆ\{-1\}{\textbackslash}textbackslash,{\textbackslash}textbackslashrm\{Mpc\}ˆ\{-1\}\$, and \${\textbackslash}textbackslashOmega\_\{{\textbackslash}textbackslashrm m\}=0.303ˆ\{+0.016\}\_\{-0.018\}\$. Combining with \${\textbackslash}textbackslashtextit\{Planck\}\$ CMB power spectrum data, we measure \${\textbackslash}textbackslashsigma\_8\$ to better than \$1{\textbackslash}textbackslash,{\textbackslash}textbackslash\%\$ precision, finding \${\textbackslash}textbackslashsigma\_8=0.811{\textbackslash}textbackslashpm 0.006\$. We find consistency with the lensing results from the Dark Energy Survey, and give combined lensing-only parameter constraints that are tighter than joint results using galaxy clustering. Using \${\textbackslash}textbackslashtextit\{Planck\}\$ cosmic infrared background (CIB) maps we make a combined estimate of the lensing potential over \$60{\textbackslash}textbackslash,{\textbackslash}textbackslash\%\$ of the sky with considerably more small-scale signal. We demonstrate delensing of the \${\textbackslash}textbackslashtextit\{Planck\}\$ power spectra, detecting a maximum removal of \$40{\textbackslash}textbackslash,{\textbackslash}textbackslash\%\$ of the lensing-induced power in all spectra. The improvement in the sharpening of the acoustic peaks by including both CIB and the quadratic lensing reconstruction is detected at high significance (abridged).},
	urldate = {2018-11-28},
	journal = {arXiv:1807.06210 [astro-ph]},
	author = {Planck Collaboration VIII},
	month = jul,
	year = {2018},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	file = {arXiv\:1807.06210 PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/VW4AM7XI/Planck Collaboration i in. - 2018 - Planck 2018 results. VIII. Gravitational lensing.pdf:application/pdf;arXiv.org Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/2CP483KV/1807.html:text/html}
}

@article{smoot_structure_1992,
