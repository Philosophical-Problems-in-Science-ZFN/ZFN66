@article{goldstein_long-time_2010,
	title = {Long-{Time} {Behavior} of {Macroscopic} {Quantum} {Systems}: {Commentary} {Accompanying} the {English} {Translation} of {John} von {Neumann}'s 1929 {Article} on the {Quantum} {Ergodic} {Theorem}},
	volume = {35},
	issn = {2102-6459, 2102-6467},
	shorttitle = {Long-{Time} {Behavior} of {Macroscopic} {Quantum} {Systems}},
	url = {http://arxiv.org/abs/1003.2129},
	doi = {10.1140/epjh/e2010-00007-7},
	abstract = {The renewed interest in the foundations of quantum statistical mechanics in recent years has led us to study John von Neumann's 1929 article on the quantum ergodic theorem. We have found this almost forgotten article, which until now has been available only in German, to be a treasure chest, and to be much misunderstood. In it, von Neumann studied the long-time behavior of macroscopic quantum systems. While one of the two theorems announced in his title, the one he calls the "quantum H-theorem", is actually a much weaker statement than Boltzmann's classical H-theorem, the other theorem, which he calls the "quantum ergodic theorem", is a beautiful and very non-trivial result. It expresses a fact we call "normal typicality" and can be summarized as follows: For a "typical" finite family of commuting macroscopic observables, every initial wave function \${\textbackslash}psi\_0\$ from a micro-canonical energy shell so evolves that for most times \$t\$ in the long run, the joint probability distribution of these observables obtained from \${\textbackslash}psi\_t\$ is close to their micro-canonical distribution.},
	number = {2},
	urldate = {2016-04-06},
	journal = {The European Physical Journal H},
	author = {Goldstein, Sheldon and Lebowitz, Joel L. and Tumulka, Roderich and Zanghi, Nino},
	month = nov,
	year = {2010},
	note = {arXiv: 1003.2129},
	keywords = {Quantum Physics, Condensed Matter - Statistical Mechanics},
	pages = {173--200},
	file = {arXiv\:1003.2129 PDF:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/JI6BKAUT/Goldstein i in. - 2010 - Long-Time Behavior of Macroscopic Quantum Systems.pdf:application/pdf;arXiv.org Snapshot:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/J23E6Z34/1003.html:text/html}
