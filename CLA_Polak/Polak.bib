@article{planck_collaboration_planck_2018-2,
	title = {Planck 2018 results. {VIII}. {Gravitational} lensing [in press]},
	url = {http://arxiv.org/abs/1807.06210},
	abstract = {We present measurements of the cosmic microwave background (CMB) lensing potential using the final \${\textbackslash}textit\{Planck\}\$ 2018 temperature and polarization data. We increase the significance of the detection of lensing in the polarization maps from \$5{\textbackslash},{\textbackslash}sigma\$ to \$9{\textbackslash},{\textbackslash}sigma\$. Combined with temperature, lensing is detected at \$40{\textbackslash},{\textbackslash}sigma\$. We present an extensive set of tests of the robustness of the lensing-potential power spectrum, and construct a minimum-variance estimator likelihood over lensing multipoles \$8 {\textbackslash}le L {\textbackslash}le 400\$. We find good consistency between lensing constraints and the results from the \${\textbackslash}textit\{Planck\}\$ CMB power spectra within the \${\textbackslash}rm\{{\textbackslash}Lambda CDM\}\$ model. Combined with baryon density and other weak priors, the lensing analysis alone constrains \${\textbackslash}sigma\_8 {\textbackslash}Omega\_\{{\textbackslash}rm m\}{\textasciicircum}\{0.25\}=0.589{\textbackslash}pm 0.020\$ (\$1{\textbackslash},{\textbackslash}sigma\$ errors). Also combining with baryon acoustic oscillation (BAO) data, we find tight individual parameter constraints, \${\textbackslash}sigma\_8=0.811{\textbackslash}pm0.019\$, \$H\_0=67.9\_\{-1.3\}{\textasciicircum}\{+1.2\}{\textbackslash},{\textbackslash}text\{km\}{\textbackslash},{\textbackslash}text\{s\}{\textasciicircum}\{-1\}{\textbackslash},{\textbackslash}rm\{Mpc\}{\textasciicircum}\{-1\}\$, and \${\textbackslash}Omega\_\{{\textbackslash}rm m\}=0.303{\textasciicircum}\{+0.016\}\_\{-0.018\}\$. Combining with \${\textbackslash}textit\{Planck\}\$ CMB power spectrum data, we measure \${\textbackslash}sigma\_8\$ to better than \$1{\textbackslash},{\textbackslash}\%\$ precision, finding \${\textbackslash}sigma\_8=0.811{\textbackslash}pm 0.006\$. We find consistency with the lensing results from the Dark Energy Survey, and give combined lensing-only parameter constraints that are tighter than joint results using galaxy clustering. Using \${\textbackslash}textit\{Planck\}\$ cosmic infrared background (CIB) maps we make a combined estimate of the lensing potential over \$60{\textbackslash},{\textbackslash}\%\$ of the sky with considerably more small-scale signal. We demonstrate delensing of the \${\textbackslash}textit\{Planck\}\$ power spectra, detecting a maximum removal of \$40{\textbackslash},{\textbackslash}\%\$ of the lensing-induced power in all spectra. The improvement in the sharpening of the acoustic peaks by including both CIB and the quadratic lensing reconstruction is detected at high significance (abridged).},
