@article{hamann_sterile_2011,
	title = {Sterile neutrinos with {eV} masses in cosmology — {How} disfavoured exactly?},
	volume = {2011},
	issn = {1475-7516},
	url = {http://stacks.iop.org/1475-7516/2011/i=09/a=034},
	doi = {10.1088/1475-7516/2011/09/034},
	abstract = {We study cosmological models that contain sterile neutrinos with eV-range masses as suggested by reactor and short-baseline oscillation data. We confront these models with both precision cosmological data (probing the CMB decoupling epoch) and light-element abundances (probing the BBN epoch). In the minimal ΛCDM model, such sterile neutrinos are strongly disfavoured by current data because they contribute too much hot dark matter. However, if the cosmological framework is extended to include also additional relativistic degrees of freedom beyond the three standard neutrinos and the putative sterile neutrinos, then the hot dark matter constraint on the sterile states is considerably relaxed. A further improvement is achieved by allowing a dark energy equation of state parameter w {\textless} −1. While BBN strongly disfavours extra radiation beyond the assumed eV-mass sterile neutrino, this constraint can be circumvented by a small ν e degeneracy. Any model containing eV-mass sterile neutrinos implies also strong modifications of other cosmological parameters. Notably, the inferred cold dark matter density can shift up by 20–75\% relative to the standard ΛCDM value.},
	language = {en},
	number = {09},
	urldate = {2018-11-28},
	journal = {Journal of Cosmology and Astroparticle Physics},
	author = {Hamann, Jan and Hannestad, Steen and Raffelt, Georg G. and Wong, Yvonne Y. Y.},
	year = {2011},
	pages = {034},
	file = {Przesłana wersja:/home/pawel/.zotero/zotero/2felkxqq.default/zotero/storage/6KDH8HT6/Hamann i in. - 2011 - Sterile neutrinos with eV masses in cosmology — Ho.pdf:application/pdf}
}

